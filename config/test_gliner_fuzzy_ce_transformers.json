{
  "loader": {"name": "text"},
  "ner": {
    "name": "gliner",
    "params": {
      "model_name": "numind/NuNER_Zero-span",
      "labels": ["person", "organization", "location"],
      "threshold": 0.5
    }
  },
  "candidate_generator": {
    "name": "fuzzy",
    "params": {"top_k": 64}
  },
  "reranker": {
    "name": "cross_encoder",
    "params": {
      "model_name": "tomaarsen/Qwen3-Reranker-4B-seq-cls",
      "top_k": 10
    }
  },
  "disambiguator": {
    "name": "transformers",
    "params": {
      "model_name": "Qwen/Qwen3-8B",
      "disable_thinking": true,
      "add_none_candidate": true
    }
  },
  "knowledge_base": null,
  "cache_dir": ".ner_cache",
  "batch_size": 1
}
